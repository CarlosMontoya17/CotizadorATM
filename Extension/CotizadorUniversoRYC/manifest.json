{
	"manifest_version": 2,
	"name": "PRECALIFICACION RYC",
	"description": "Software de precalificacion infonavit",
	"version": "1.0",
	"author": "Carlos Montoya",
        

	"icons": {
		
		"64": "images/icons/64.png",
		"128": "images/icons/128.png"
	},

	"content_scripts": [
        {
            "matches": ["https://www.mi-portal-infonavit.com/*"],
            "js": ["start.js", "socket-io.js"]
        }
    ],

	"permissions": [
          "storage","tabs","https://www.mi-portal-infonavit.com/*"
    ]
}
